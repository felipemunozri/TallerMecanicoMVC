
@{
    ViewBag.Title = "ModificarProveedor";
}

<h2>Modificar Proveedor</h2>

<!-- Panel Proveedor -->
<div class="row">

    <div class="col-lg-12 ui-sortable">

        <div class="panel panel-inverse" data-sortable-id="form-plugins-1">

            <div class="panel-heading">
                <h4 class="panel-title">Buscar Proveedor</h4>
                <div class="panel-heading-btn">
                    <!--<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-default" data-click="panel-expand"><i class="fa fa-expand"></i></a>-->
                    <!--<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-success" data-click="panel-reload"><i class="fa fa-redo"></i></a>-->
                    <a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-warning" data-click="panel-collapse"><i class="fa fa-minus"></i></a>
                    <!--<a href="javascript:;" class="btn btn-xs btn-icon btn-circle btn-danger" data-click="panel-remove"><i class="fa fa-times"></i></a>-->
                </div>
            </div>
            <div class="panel-body">
                <div class="table-responsive form-inline">
                    <table class="table table-profile">
                        <tbody>
                            <tr class="highlight">
                                <td>Rut Proveedor</td>
                                <td>
                                    <input type="text" name="txtRutProveedor" id="txtRutProveedor" class="form-control" maxlength="10" tabindex="1" />
                                </td>
                                <td>
                                    <input class="btn btn-success" type="button" name="btnBuscar" value="Buscar">
                                </td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr class="highlight">
                                <td>Nombre</td>
                                <td>
                                    <input type="text" name="txtNomProveedor" id="txtNomProveedor" class="form-control" maxlength="30" tabindex="2" />
                                </td>
                                <td>Teléfono</td>
                                <td>
                                    <input type="number" name="txtTelProveedor" id="txtTelProveedor" class="form-control" min="0" max="999999999" tabindex="3" />
                                </td>
                                <td>Email</td>
                                <td>
                                    <input type="email" name="txtMailProveedor" id="txtMailProveedor" class="form-control" maxlength="50" tabindex="4" />
                                </td>
                            </tr>
                            <tr class="highlight">
                                <td>Dirección</td>
                                <td>
                                    <input type="text" name="txtDirProveedor" id="txtDirProveedor" class="form-control" maxlength="200" tabindex="5" />
                                </td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr class="highlight">
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                                <td></td>
                            </tr>
                            <tr class="highlight">
                                <td></td>
                                <td></td>
                                <td></td>
                                <td>
                                    <input class="btn btn-primary" type="button" name="btnModProveedor" value="Modificar Proveedor">
                                </td>
                                <td></td>
                                <td></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>